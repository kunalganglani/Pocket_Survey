<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kunalganglani.com.suveyji.DataDetail"
    android:background="@drawable/formbackground">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/formtitle" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

    <TextView
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:ellipsize="start"
        android:gravity="center"
        android:background="@drawable/edittextstyle"
        android:hint="Name"
        android:id="@+id/Namelbl"
        android:layout_below="@+id/etEducationalqualification"
        android:layout_marginTop="40dp"
        />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:ellipsize="start"
        android:gravity="center"
        android:background="@drawable/edittextstyle"
        android:id="@+id/Nameval"
        android:layout_below="@+id/Namelbl"
        android:layout_marginTop="0dp"
        />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:hint="Age"
            android:id="@+id/Agelbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:hint="Name"
            android:id="@+id/Ageval"
            android:layout_marginTop="0dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:hint="Gender"
            android:id="@+id/Genderlbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Genderval"
            android:layout_marginTop="0dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:hint="Education"
            android:id="@+id/Educationlbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Educationval"
            android:layout_marginTop="0dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:hint="Status"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Presentstatuslbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Presentstatusval"
            android:layout_marginTop="0dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:hint="Interested?"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Interestlbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Interestval"
            android:layout_marginTop="0dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:hint="Place"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Placelbl"
            android:layout_marginTop="40dp"
            />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:ellipsize="start"
            android:gravity="center"
            android:background="@drawable/edittextstyle"
            android:id="@+id/Placeval"
            android:layout_marginTop="0dp"
            />


    </LinearLayout>
    </ScrollView>
</RelativeLayout>
